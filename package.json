{
  "name": "@palisadeinc/identity-sdk",
  "version": "1.2.1",
  "description": "Palisade Identity SDK",
  "main": "dist/palisade-sdk.cjs.js",
  "module": "dist/palisade-sdk.esm.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/palisade-sdk.esm.js",
      "require": "./dist/palisade-sdk.cjs.js"
    }
  },
  "files": [
    "dist/",
    "types/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "esbuild src/index.js --bundle --minify --format=cjs --outfile=dist/palisade-sdk.cjs.js",
    "build:esm": "esbuild src/index.js --bundle --minify --format=esm --outfile=dist/palisade-sdk.esm.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "PalisadeInc",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.25.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palisadeinc/identity-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/palisadeinc/identity-sdk/issues"
  },
  "homepage": "https://github.com/palisadeinc/identity-sdk#readme"
}
